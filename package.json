{
  "name": "patternplate-client",
  "description": "Universal client-application for patternplate-server",
  "main": "distribution/library/",
  "scripts": {
    "start": "node ./distribution/binary/patternplate-client.js",
    "gulp": "gulp",
    "build": "gulp build",
    "watch": "gulp watch",
    "test": "gulp test",
    "zuul": "gulp zuul",
    "cover": "istanbul cover distribution/test/index.js",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "pretest": "node distribution/scripts/when-ci.js 'node' 'distribution/scripts/validate-commit.js'",
    "posttest": "node distribution/scripts/when-ci.js 'node' 'distribution/scripts/posttest.js'",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "bin": {
    "patternplate-client": "distribution/binary/patternplate-client.js"
  },
  "repository": {
    "type": "git",
    "slug": "sinnerschrader/patternplate-client",
    "url": "https://github.com/sinnerschrader/patternplate-client.git"
  },
  "publishConfig": {
    "tag": "next"
  },
  "release": {
    "branch": "next"
  },
  "keywords": [
    "atomic",
    "atomic design",
    "tool",
    "universal",
    "isomorphic",
    "react",
    "patternplate",
    "patternplate-client"
  ],
  "author": {
    "name": "SinnerSchrader",
    "email": "jobs@sinnerschrader.com"
  },
  "contributors": [
